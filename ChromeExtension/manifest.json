{
  "manifest_version": 2,
  "name": "Mediaplayer",
  "version": "0.1",
  "icons": { "128": "icon.png" },
    
  "content_scripts": 
   [
	  {
		"matches": [
		  "<all_urls>"
		],
		"js": ["jquery.js", "main.js"]
	  }
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Test",
		"default_popup": "popup.html"
	  },
	"permissions": [
		"activeTab"
	]
}