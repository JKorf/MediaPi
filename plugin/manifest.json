{
  "name": "Homebase plugin",
  "description": "Plugin to play videos on homebase",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
	"webNavigation",
	"<all_urls>",
	"tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
